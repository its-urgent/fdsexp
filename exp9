#include<iostream>
#include<string>
#include<stack>
using namespace std;

bool isTrue(const string&exp){
    stack<char> pare;

    for(char ch : exp){
        if(ch=='(' || ch=='{' || ch=='[' ){
            pare.push(ch);
        }else if(ch==')' || ch=='}' || ch==']'){
            if(pare.empty()){
                return false;
            }
            char top = pare.top();
            pare.pop();
            if((ch==')'&&top!='(') || (ch=='}'&&top!='{') || (ch==']'&&top!='[')){
                return false;
            }
        }
    }
    return pare.empty();
}

int main(){
    string exp;
    //take input as string
    cout << "Enter exp : ";
    getline(cin,exp);
    //checking 
    if(isTrue(exp)){
        cout << "Well Paranthesised!!";
    }else{
        cout << "Not well parenthesis!!";
    }
    return 0;
}
